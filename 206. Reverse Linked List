# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def reverseList(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        tail = head
        result = None
        while tail is not None:
            if result is None:
                result = tail
                tail = tail.next
                result.next = None
            else:
                temp = tail
                tail = tail.next
                temp.next = result
                result = temp
        return result
                
        
